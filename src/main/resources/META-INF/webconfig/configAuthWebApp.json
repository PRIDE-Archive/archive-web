{
    "localMasthead": {
        "style": "logo-title",
        "title": "PRIDE Archive",
        "homepageUrl": "${deploy.url}",
        "logo": {
            "src": "//www.ebi.ac.uk/pride/resources/logos/pride_logo.jpg",
            "alt": "logo",
            "width": "64",
            "height": "64"
        },
        "search": {
            "btnText": "Search",
            "action": "${deploy.url}simpleSearch",
            "method": "get",
            "inputName": "q",
            "examples": [
                {
                    "title": "stress",
                    "href": "${deploy.url}simpleSearch?q=stress"
                },
                {
                    "title": "human",
                    "href": "${deploy.url}simpleSearch?q=human"
                },
                {
                    "title": "blood",
                    "href": "${deploy.url}simpleSearch?q=blood"
                },
                {
                    "title": "P02768",
                    "href": "${deploy.url}simpleSearch?q=P02768"
                },
                {
                    "title": "MDPNTIIEALR*",
                    "href": "${deploy.url}simpleSearch?q=MDPNTIIEALR*"
                }
            ]
        },
        "navigationTabs": [
            {
                "selected": false,
                "title": "Home",
                "id": "home-menu",
                "href": "${deploy.url}",
                "tooltip": "Home page"
            },
            {
                "selected": false,
                "title": "Submit data",
                "id": "submit-menu",
                "href": "//www.ebi.ac.uk/pride/help/archive/submission",
                "tooltip": "Submit data to PRIDE Archive"
            },
            {
                "selected": false,
                "title": "Browse data",
                "id": "browse-menu",
                "href": "${deploy.url}simpleSearch",
                "tooltip": "Browse data in PRIDE Archive"
            },
            {
                "selected": false,
                "title": "Help",
                "id": "help-menu",
                "href": "//www.ebi.ac.uk/pride/help/archive",
                "tooltip": "PRIDE Archive help"
            },
            {
                "selected": false,
                "title": "Publications",
                "id": "pub-menu",
                "href": "//scholar.google.co.uk/citations?user=tzHv1ZcAAAAJ&hl=en",
                "tooltip": "PRIDE Publications"
            },
            {
                "selected": false,
                "title": "About PRIDE Archive",
                "id": "about-prider-menu",
                "href": "//www.ebi.ac.uk/pride/help/archive/about",
                "tooltip": "About PRIDE Archive"
            }
        ],
        "functionalTabs": [
            {
                "title": "Feedback",
                "href": "//www.ebi.ac.uk/support/index.php?query=pride",
                "iconFontSet": "icon-static",
                "dataIcon": "\\"
            },
            {
                "selected": false,
                "title": "Profile",
                "id": "profile-menu",
                "href": "${deploy.url}users/profile",
                "tooltip": "User profile",
                "iconFontSet": "icon-static",
                "dataIcon": "M"
            },
            {
                "selected": false,
                "title": "Logout",
                "id": "logout-menu",
                "href": "${deploy.url}logout",
                "tooltip": "Logout",
                "iconFontSet": "icon-functional",
                "dataIcon": "{"
            }
        ]
    },
    "globalFooter": true,
